@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
<header class="page-header">

    <!-- [Homepage Carousell] -->

    <div class="col-md-8">@Html.Sitecore().Placeholder("breadcrumb-ph")</div>
    <!-- [/Homepage Carousell] -->

</header>

<section class="section section-full">
    <div class="container">
        <div class="col-md-12" style="text-align:center;position:center;">
            <div class="col-md-12">
                <div class="col-md-12" style="text-align:center;position:center;">
                    <h1>@Html.Sitecore().Field("ContentSubheading")</h1>
                </div>
                <h1 style="display:block;margin-left: auto;margin-right: auto;">
                    @Html.Sitecore().Field("DecorationBanner")
                </h1>
            </div>
        </div>
        <hr />
        <div class="col-md-12" style="text-align:center;position:center;">
            <div class="well">@Html.Sitecore().Field("ContentIntro")</div>
        </div>
    </div>
    <p><h1>More Stories</h1></p>
    <span> @Html.Sitecore().Placeholder("carousel-slides")</span>
</section>
