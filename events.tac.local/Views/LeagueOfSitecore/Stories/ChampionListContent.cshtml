@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}

<!-- [Home Content] -->
<header class="page-header">
    <!-- [Homepage Carousell] -->
    <div class="col-lg-8">@Html.Sitecore().Placeholder("breadcrumb-ph")</div>
    <!-- [/Homepage Carousell] -->
</header>
<section class="section section-full">
    <div class="container">
        <div class="col-lg-12 text-center">
            <h4>@Html.Sitecore().Field("Sectionheader")</h4>
            <h1>@Html.Sitecore().Field("ContentHeading")</h1>
        </div>
        <div class="col-md-12 text-center">
            <div class="col-md-1">

            </div>
            <div class="col-md-10">
                @Html.Sitecore().Field("DecorationBanner", new { @class = "img-responsive thumbnail ", mw = 1000, mh = 666, DisableWebEdit = true })
            </div>
            <div class="col-md-1"></div>
        </div>
        <div class="col-md-12 text-center">
            <div class="col-md-2"></div>
            <div class="col-md-8 text-left">
                <h3 style="line-height: 1.6; color: #c4b998"> @Html.Sitecore().Field("ContentIntro")</h3>
            </div>
            <div class="col-md-2"></div>
        </div>
    </div>
    <p><h1>More Stories</h1></p>
    <span> @Html.Sitecore().Placeholder("carousel-slides")</span>
</section>